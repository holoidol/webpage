package com.company;

public class Point {


    private int x;
    private int y;

    public Point(int x, int y) {
        this.x = x;
        this.y = y;
    }

    public Point() {
    }

    public int getX() {
        return x;
    }

    public int getY() {
        return y;
    }

    public void setX(int x) {
        this.x = x;
    }

    public void setY(int y) {
        this.y = y;
    }

    public double distance(){
        double dist=(0-x)*(0-x)+(0-y)*(0-y);
        double sqr=Math.sqrt(dist);
        
        return sqr;
    }
    ///leaving off here
    public double distance(int x, int y){
        this.y=y;
        this.x=x;
        return this.x-this.y;
    }

    public double distance(int point){
        return point-
    }
}
